# PostgreSQL Database Configuration
NUDLERS_DB_USER=myuser
NUDLERS_DB_HOST=nudlers-db
NUDLERS_DB_NAME=nudlers
NUDLERS_DB_PASSWORD=mypassword
NUDLERS_DB_PORT=5432

# Encryption Key (Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
NUDLERS_ENCRYPTION_KEY=

# Application Authentication Password
NUDLERS_AUTH_PASSWORD=password

# Google Gemini API Key (Get from: https://makersuite.google.com/app/apikey)
GEMINI_API_KEY=

# Enable VNC for browser debugging in Docker (access at http://localhost:6080/vnc.html)
# Set to "true" to enable, useful for debugging scrapers or completing 2FA
ENABLE_VNC=false
